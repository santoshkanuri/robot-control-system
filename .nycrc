 {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
        "src/!(test).[tj]s?(x)"
    ],
    "exclude": [
        "src/tests/*.ts",
        "src/index.ts"
    ],
    "reporter": [
        "html",
        "lcov",
        "text",
        "text-summary"
    ],
    "report-dir": "coverage"

}